<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <path>rpm.ac</path>
  
  <animation>
    <!-- Objets opaques -->
    <object-name>RPMBezel</object-name>
    <object-name>RPMFace</object-name>
    <object-name>RPMNeedle</object-name>
    <object-name>RPMVernier</object-name>
    
    <!-- Objets transparents -->
    <object-name>vitre</object-name>
  </animation>
  
  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Aircraft/Super-Frelon/Models/Effects/Glass/glass</inherits-from>
    <object-name>vitre</object-name>
  </effect>
  
  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>vitre</object-name>
  </effect>
  
  <animation>
    <type>material</type>
    <object-name>RPMFace</object-name>
    <object-name>RPMNeedle</object-name>
    <object-name>RPMVernier</object-name>
    <emission>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission> 
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>RPMNeedle</object-name>
    <property>/engines/engine[0]/n1</property>
    <interpolation>
      <entry><ind>   0 </ind><dep>   0 </dep></entry>
      <entry><ind> 100 </ind><dep> 275 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>RPMNeedle</object-name>
    <property>/engines/engine[0]/n2</property>
    <interpolation>
      <entry><ind>   0 </ind><dep>   0 </dep></entry>
      <entry><ind> 100 </ind><dep> 275 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.00000 </x-m>
      <y-m> -0.00716 </y-m>
      <z-m>  0.00915 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
</PropertyList>
